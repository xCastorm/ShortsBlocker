{
  "manifest_version": 3,
  "name": "YouTube Shorts Redirect",
  "version": "1.0",
  "description": "Redirects YouTube Shorts every 5 seconds.",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["redirect.js"],
      "run_at": "document_start"
    }
  ]
}
